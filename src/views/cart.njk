{% extends "layouts/layout.njk" %}
{% block content %}
<div>
    {%if context.cart and context.cart.items %}
    <div class=" flex flex-col gap-10 container md:px-4">
        <div class=" flex flex-col gap-2">
            <h1 class=" font-bold text-2xl text-customBlack">{{context.page.title}}</h1>
            <p class=" text-subColor">{{context.page.description}}</p>
        </div>
        <div class=" flex md:flex-row flex-col gap-[43px] py-5">
            <div style="border-radius: 20px;" class=" bg-white initial-scroll  md:w-2/3   flex flex-col gap-6">
                {%for item in context.cart.items%}

                <div
                    class=" w-full  grid grid-cols-3   items-center  rounded-lg gap-4 shadow-[0px_0px_25px_#D7D7D780] p-[20px]">

                    <div class=" flex gap-4  col-span-2">
                        <img src="{{item.product.images[0].imageUrl}}"
                            style="width: 80px;height: 80px; object-fit: contain;" alt="">
                        <div class=" flex flex-col  justify-center ">
                            <div class="text-customBlack">
                                {% if item.product.title.length > 18 %}
                                {{ item.product.title.slice(0, 18) }}...
                                {% else %}
                                {{ item.product.title }}
                                {% endif %} </div>
                            <div class=" text-[#929292]">
                                bmbmbn
                                {% if item.properties and item.properties|length > 0 %}
                                {% for prop in item.properties %}
                                {% if prop.property and prop.value %}
                                <span>{{ prop.property.title }} : {{ prop.value.title }}</span>
                                {% endif %}
                                {% endfor %}
                                {% endif %}
                            </div>

                        </div>
                    </div>

                    <qumra-quantity-input value=1></qumra-quantity-input>

                    <div class="text-customBlack justify-self-end font-[600]">{{item.product.price|money}}</div>
                    <div class="justify-self-end ">
                        <qumra-dropdown 
                        icon='<svg class="cursor-pointer" width="24" height="6" viewBox="0 0 24 6" fill="none" xmlns="http://www.w3.org/2000/svg">
                                 <circle cx="3" cy="3" r="3" fill="#C6C6C6" />
                                 <circle cx="12" cy="3" r="3" fill="#C6C6C6" />
                                 <circle cx="21" cy="3" r="3" fill="#C6C6C6" />
                               </svg>'>
                        <div class="w-[137px]">
                            <div class="text-xs hover:text-mainColor gap-1 cursor-pointer flex  items-center">
                                <svg class="cursor-pointer over:text-mainColor " xmlns="http://www.w3.org/2000/svg" width="14px" height="14px" viewBox="0 0 1024 1024" class="icon" version="1.1" fill="#000000"><script xmlns=""/><g id="SVGRepo_bgCarrier" stroke-width="0"/><g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"/><g id="SVGRepo_iconCarrier"><path d="M960 160h-291.2a160 160 0 0 0-313.6 0H64a32 32 0 0 0 0 64h896a32 32 0 0 0 0-64zM512 96a96 96 0 0 1 90.24 64h-180.48A96 96 0 0 1 512 96zM844.16 290.56a32 32 0 0 0-34.88 6.72A32 32 0 0 0 800 320a32 32 0 1 0 64 0 33.6 33.6 0 0 0-9.28-22.72 32 32 0 0 0-10.56-6.72zM832 416a32 32 0 0 0-32 32v96a32 32 0 0 0 64 0v-96a32 32 0 0 0-32-32zM832 640a32 32 0 0 0-32 32v224a32 32 0 0 1-32 32H256a32 32 0 0 1-32-32V320a32 32 0 0 0-64 0v576a96 96 0 0 0 96 96h512a96 96 0 0 0 96-96v-224a32 32 0 0 0-32-32z" fill="#231815"/><path d="M384 768V352a32 32 0 0 0-64 0v416a32 32 0 0 0 64 0zM544 768V352a32 32 0 0 0-64 0v416a32 32 0 0 0 64 0zM704 768V352a32 32 0 0 0-64 0v416a32 32 0 0 0 64 0z" fill="#231815"/></g><script xmlns=""/></svg>
                                حذف
                            </div>
                        </div>
                      </qumra-dropdown>
                    </div>

                </div>
                {%endfor%}

            </div>

            <div class="md:w-1/3 flex flex-col items-center gap-6">

                <div class="bg-white w-full shadow-[0px_0px_25px_#D7D7D780]  rounded-lg h-max "
                    style="padding:16px 26px 25px ;">
                    <div class=" flex flex-col gap-8">
                        <div class=" flex flex-col gap-4">
                            <div class=" flex flex-col gap-4">
                                <div class="font-bold text-customBlack">
                                    اجمالي الطلب
                                </div>
                                <div class=" border-b border-dashed flex w-full"></div>
                            </div>
                            <div class=" flex flex-col gap-3">
                                <div class=" flex justify-between">
                                    <span class=" font-bold  text-subColor text-base">اجمالي المنتجات</span>
                                    <span class=" font-bold text-customBlack">
                                        {% if context.cart.totalProducts > 0 %}
                                        {{context.cart.totalProducts | money}}
                                        {%else%}
                                        0
                                        {%endif%}
                                    </span>
                                </div>
                                <div class=" flex justify-between">
                                    <span class=" font-bold  text-subColor text-base">الخصم</span>
                                    <span class=" font-bold text-customBlack">
                                        {%if context.cart.productDiscount > 0%}
                                        {{ context.cart.productDiscount|money}}
                                        {%else%}
                                        0
                                        {%endif%}
                                    </span>
                                </div>
                                <div class=" flex justify-between">
                                    <span class=" font-bold  text-subColor text-base">سعر المنتحات النهائي</span>
                                    <span class=" font-bold text-customBlack">
                                        {%if context.cart.totalProductsAfterDiscount >0%}

                                        {{context.cart.totalProductsAfterDiscount|money}}
                                        {%else%}
                                        0
                                        {%endif%}

                                    </span>
                                </div>
                            </div>
                            <div class=" border-b border-dashed flex w-full"></div>
                            <div class=" flex justify-between">
                                <span class=" font-bold  text-subColor text-base">الإجمالي</span>
                                <span class=" font-bold text-mainColor text-customBlack">
                                    {%if context.cart.totalPrice >0%}

                                    {{context.cart.totalPrice|money}}
                                    {%else%}
                                    0
                                    {%endif%}

                                </span>
                            </div>
                        </div>

                        <qumra-checkout-button
                            class="py-3 mt-2 flex flex-col gap-8 rounded-lg text-white  bg-brown w-full">
                            ادفع الان</qumra-checkout-button>
                    </div>
                </div>
                <div class=" relative w-full ">
                    <svg  width="363" height="190" viewBox="0 0 363 190" fill="none" xmlns="http://www.w3.org/2000/svg">
